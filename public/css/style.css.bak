}
.link:hover{
    color: var(--olympic-blue) !important;
}
.dropdown-menu{
    border-radius: 0px;
    padding: 0px;
}

.form-control{
    border-radius: 0px;
}

.textarea{
    -moz-appearance: textfield-multiline;
    -webkit-appearance: textarea;
    font: medium -moz-fixed;
    font: -webkit-small-control;
    min-height: 80px;
    overflow: auto;
    width: 100%;
    max-height: 250px;
}
.textarea .helpText{
    color:#999;
    font-size: 20px;
}
.inputDiv{
    -moz-appearance: textfield-multiline;
    -webkit-appearance: textarea;
    font: medium -moz-fixed;
    font: -webkit-small-control;
    line-height: 35px;
    height: 35px;
    width: 100%;
    white-space: nowrap;
    overflow: hidden;
}
.inputDiv .helpText{
    color:#ccc;
    font-size: 16px;
}

/*Modal CSS*/
.modal-header , .modal-footer , .modal-content{
    border-radius: 0px !important;
    background-clip:border-box;
}

.modal-header , .modal-footer{
    height: 40px;
    align-items: center;
    padding: 0px 1rem;
}
.modal-footer{
    justify-content: space-between !important;
}
.modal-header .close{
    padding: 0px;
    width: 40px;
    height: 40px;
    line-height: 40px;
    text-align: center;
}
.modal-header .modal-title{
    font-weight: normal;
}
.modal-dialog{
    margin-top: 64px;
}
.modal-md{
    max-width: 520px;
}
.modal-open .mainBody , .modal-open #header{
    filter:blur(2px);
}

/*Select Picker CSS*/

.bootstrap-select>.dropdown-toggle:focus , .bootstrap-select>.dropdown-toggle:active{
    outline: none!important;
    box-shadow: none !important;
    border-radius: 0px;
}
.bootstrap-select>.dropdown-toggle{
    height: 38px !important;
    border-radius: 0px;
}



#header {
    /*height: 100px;*/ 
    background: #fff;
    position: relative;
    z-index: 5;
    box-shadow: 0px 1px 2px rgba(0,0,0,0.06);
}
.blankHeader{
    height: 94px;
}
.blankHeader .topMenu{
    display: none;
}
.logo{ 
    width: 200px;
}
.logo img{ height: 35px;}
.topMenu{ width:calc(100% - 200px); text-align: right; padding-top: 5px; padding-bottom: 5px;}


.topMenu .upperContainer{
    margin-bottom: 5px;

}
.topMenu .upperContainer>ul , .topMenu .upperContainer>ul>li{display: inline-block; }
.topMenu .upperContainer>ul>li>a{ color: var(--light-grey-txt);}


.topMenu .lowerContainer>ul , .topMenu .lowerContainer>ul>li{display: inline-block; }
.topMenu .lowerContainer>ul>li>a{ color: var(--light-grey-txt); position: relative;}
.topMenu .lowerContainer>ul>li>a span{display: block;}
.topMenu .lowerContainer>ul>li>a span.badge{
    display: inline-block;
    position: absolute;
    top: 5px;
    right: 25px;
}


.userBtn { width:35px; height: 35px; padding: 0px;}
.userBtn:hover , .userBtn:focus , .userBtn:active{background: transparent !important;}
.userBtn img{ width:35px; height: 35px; border-radius: 50%;}


.searchInput{
    width: 250px;
    position: relative;
}
.searchInput input{
    padding-right: 25px;
    height: 25px;
}
.searchInput .icon{
    width:25px;
    height: 25px;
    line-height: 25px;
    text-align: center;
    position: absolute;
    right: 0px;
    top: 0px;

}
.searchInput .icon i:before{
    font-size: 14px;
    color: var(--light-grey-txt)
}


.btn{ margin-bottom: 0px;}
[class^="flaticon-"]:before, [class*=" flaticon-"]:before, [class^="flaticon-"]:after, [class*=" flaticon-"]:after {margin-left: 0px; font-size: 18px;}


.mainBody{ padding-top: 15px; padding-bottom: 15px;}
.mainBody .middle-container{padding:0 15px;}
.box{background: #fff;}
.box + .box{margin-top: 15px;}
.box .box-header{ height: 40px; padding: 0 10px; display: flex; align-items: center; justify-content: space-between;}

.box .box-header .title{font-size: 12px; font-family: 'Roboto-Bold'; color:#999;}
.box .box-body{ padding: 10px;}
.box .box-footer{ padding: 10px;}
.box .box-header.with-border{ 
    background: #f2f2f2;
    border-bottom: 1px solid #dedede;
}



.iconList{ margin: 0px; padding: 0px; display: flex; list-style: none;}
.iconList>li + li{
    padding-left: 5px;
}
.iconList>li>a{ display: block; height: 40px; width: 40px; line-height: 40px; text-align: center;}
.iconList>li>a:hover{background: #f9f9f9;}

.userContainer .header{ height: 70px;}
.userContainer .profileBody{ position: relative;}
.userContainer .profileBody .userImg{position: absolute; left: 15px; top: -30%; border-radius: 50%; background: #fff; height: 15px; width: 60px; height: 60px; border: 2px solid #fff;}
.userContainer .profileBody .userInfo{padding-left: 80px;}
.userContainer .profileBody .userImg{position: absolute; left: 15px; top: -30%; border-radius: 50%; background: #fff; height: 15px; width: 60px; height: 60px; border: 2px solid #fff;}
.userContainer .profileBody .userInfo .userName{font-size: 16px;}
.userContainer .profileBody .userDetails {  padding: 10px;}
.userContainer .profileBody .userDetails table{ width: 100%; text-align: center; }
.userContainer .profileBody .userDetails table tr td h5{font-size: 14px;}
.userContainer .profileBody .userDetails table tr td span{font-size: 12px; color: var(--light-grey-txt); font-family: 'Roboto-Bold';}

.userContainer.company , .userContainer.company .profileBody .userImg{
    background: transparent;
}
.userContainer.company .header{  height: 0px;}
.userContainer.company .profileBody .userInfo{
    /*padding-left: 0px;*/
    padding: 15px 2px;
    text-align: center;
    background: #fff;
    border:1px solid #dedede;
}
.userContainer.company .profileBody .userInfo .userName{font-size: 20px;}
.userContainer.company .profileBody .userImg{position: relative;  border-radius: 50%; background: #fff; width: 150px; height: 150px; border: 2px solid #fff; margin: 0 auto; display: block; margin-bottom: 15px;}

.userFollowList li{ display: flex; position: relative; flex-wrap: wrap; }
.userFollowList li+li{margin-top: 25px;}
.userFollowList li>img{ width:40px; height: 40px; border-radius: 50%; }
.userFollowList li .info { width:calc(100% - 40px); padding-left: 15px;}
.userFollowList li .info .userName{ font-size: 14px;}
/*.userFollowList li .followBtn{position: absolute; right: 0px; top: 6px;}*/
.userFollowList li .actionBtns{  padding-left: 55px; display: flex;}
.userFollowList li .actionBtns>a + a{ margin-left: 5px;}

.hashtagsList li{padding: 5px 0px;}
.hashtagsList li:before{content: '#';}
.hashtagsList li a{ font-size: 12px; color: var(--light-grey-txt);}
.hashtagsList li a:hover{ color: #666;}
.hashtagsList.inline li{ display: inline-block; margin-right: 5px;}
.list.nav {
    display: block;
}
.list li a{
    display: flex;
    padding: 8px 10px;
    border-right: 2px solid transparent;
    align-items: center;
    position: relative;
}
.list li a:hover {
    background: #f2f2f2;
    border-color: var(--olympic-blue);
}
.list.withoutHoverEffect a:hover {
    background: #ffffff;
    border-color: transparent;
}
.list.with-border li + li{
    border-top: 1px solid #f2f2f2;
}
.list li a.active{
    background: #f2f2f2;
    border-color: var(--olympic-blue);
    color: var(--olympic-blue);
}

.withIconList li a i:after , .withIconList li a i:before{
    font-size: 14px !important;

}
.withIconList li a i{
    margin-right: 5px;
    display: inline-block;
    width:15px;

}
.withIconList li a span{
    width: calc(100% - 15px);

    display: inline-block;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
.withIconList .companyImg{
    height: 30px;
    width: 30px;
    border-radius: 50%;
    border: 1px solid #dedede;
    background: #fff;
    margin-right: 10px;
}
.withIconList .trendsIcon{
    position: absolute;
    width:25px;
    height: 25px;
    line-height: 25px;
    text-align: center;
    display: inline-block;
    background: #ccc;
    border-radius: 50%;
    top: 12px;
    right: 5px;
    color: #fff;
}

.withIconList .trendsIcon.green{
    background: green;
}
.withIconList .trendsIcon.red{
    background: red;
}
.withIconList .trendsIcon.orange{
    background: orange;
}
.withIconList .trendsIcon.dark-orange{
    background: tomato;
}
.withIconList .trendsIcon.light-green{
    background: greenyellow;
}

.blogContainer  .title{ font-size: 14px;}
.blogContainer  .userInfo>div:first-child{ display: flex;}
.blogContainer  .userInfo img{ width:40px; height: 40px; border-radius: 50%; margin-right: 10px;}
.blogContainer  .userInfo .info .userName{font-size: 14px;}

.blogSection{ padding: 15px 0px 0px;}
.blogSection figure img{width:100%;}
.blogSection .blogTitle{ font-size: 16px; margin-bottom: 10px;}
.blogSection .blogSubTitle{ font-size: 14px; color: var(--light-grey-txt); font-family: 'Roboto-Regular';  margin-bottom: 5px; }
.blogSection article{ 
    display: block;
    overflow: hidden;
    display: -webkit-box;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;  
}
.blogSection a:hover , .blogSection a:focus , .blogSection a:active{ color: inherit;}

.blogFooter{ padding-top: 15px;}
.inlineList{ display: flex; font-size: 12px;}
.inlineList li { position: relative;}
.inlineList li + li{padding-left: 15px;}
.inlineList li + li:before{ content: '.'; position: absolute; font-size: 20px; left: 5px; top: -12px; }
.inlineList li a{ color: var(--light-grey-txt);}

.icon-inline-list{
    display: flex;
    height: 100%;
}
.icon-inline-list>li>a{
    display: block;
    height: 38px;
    padding: 0 10px;
    line-height: 38px;
}
.icon-inline-list>li>a:hover{
    background: #f9f9f9;
}

/*Comment CSS*/
.commentContainer{
    padding: 10px;
}
.addComment{
    display: flex;
}
.addComment .img{
    width:35px;
}
.addComment .img img{
    width:35px;
    height: 35px;
    border-radius: 50%;
}
.addComment .comment{
    width:calc(100% - 30px);
    padding-left: 10px;
}
.addComment .comment .input-group{
    border:1px solid #ccc;
}
.addComment .comment .form-control{
    border:none;
}
.addComment .comment .form-control:focus{
    box-shadow: none;
}
.addComment .comment .btn{
    border:none;
    padding: 0px 10px;
    border-radius: 0px;
    font-size: 16px;
}
.addComment .comment .btn:hover , .addComment .comment .btn:focus , .addComment .comment .btn:active{
    background: transparent;
    color:#333;
}
.userMainProfile{
    margin-bottom: 15px;
}
